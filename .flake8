[flake8]
max-line-length = 100
exclude =
    .git,
    __pycache__,
    .venv,
    venv,
    build,
    dist,
    .eggs,
    *.egg-info,
    .mypy_cache,
    .pytest_cache,
    .coverage,
    htmlcov,
    site,
    core_original_backup.py
# E203: whitespace before ':'
# W503: line break before binary operator
ignore = E203,W503
per-file-ignores =
    __init__.py:F401


